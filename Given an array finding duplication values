#include <stdio.h>

int main() {
    int size;
    printf("Enter the size of the array: ");
    scanf("%d", &size);

    int arr[size];
    printf("Enter %d elements:\n", size);
    for (int i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Duplicate elements in the array are: ");
    for (int i = 0; i < size; i++) {
        int isDuplicate = 0;
        for (int j = i + 1; j < size; j++) {
            if (arr[i] == arr[j]) {
                isDuplicate = 1;
                break;
            }
        }
        // Check if already printed
        for (int k = 0; k < i; k++) {
            if (arr[i] == arr[k]) {
                isDuplicate = 0;
                break;
            }
        }
        if (isDuplicate) {
            printf("%d ", arr[i]);
        }
    }

    return 0;
}
